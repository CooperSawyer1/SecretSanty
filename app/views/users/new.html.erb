<h2> Sign Up below! </h2>

<%= form_for @user do |form| %>

<%= form.label :name%>
<%= form.text_field :name%>
<br>
<%= form.label :email%>
<%= form.email_field :email%>
<br>
<%= form.label :username%>
<%= form.text_field :username%>
<br>
<%= form.label :password%>
<%= form.text_field :password%>
<br>
<%= form.submit %>
<% end %>